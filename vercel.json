{
  "rewrites": [
    { "source": "/css/(.*)", "destination": "/public/css/$1" },
    { "source": "/js/(.*)", "destination": "/public/js/$1" },
    { "source": "/audio/(.*)", "destination": "/public/audio/$1" },
    { "source": "/icons/(.*)", "destination": "/public/icons/$1" },
    { "source": "/legal/(.*)", "destination": "/public/legal/$1" }
  ]
}
